
public class Port {
	public String name;
	public int noOfParkingSlots;
	public int noOfBuildings;
	public String openSlots;
	
	public Port() {
		System.out.println("This is a new port.");
	}
	
	public void setNoOfBuildings(int number) {
		noOfBuildings = number;
	}
	
	public void timeForNextShipArrival() {
		System.out.println("Ship A arrives at 11:00PM");
	}
	
	public void printPortName() {
		System.out.println("The name of this port is "+ name+".");
	}

}

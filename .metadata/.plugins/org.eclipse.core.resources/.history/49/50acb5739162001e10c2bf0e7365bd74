
public class Dog {
    public String name;
    public int age;
    public String breed;
    public boolean shotStatus;
    public boolean isSleeping;
    public boolean isBarking;
    
    public void sleeping(boolean isSleeping) {
        if(isSleeping) {
        	isBarking = false;
        	System.out.println("SnooooooooooreðŸ˜´");
        }
    }
    public void bark(boolean isBarking) {
    	if(isBarking) {
    		isSleeping = false;
    		System.out.println("Woof Woof!ðŸ˜Š");
    	}
    }
}

